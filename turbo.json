{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": [],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".svelte-kit/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "extends":[
    "//"
  ]
}